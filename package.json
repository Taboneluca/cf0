  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Taboneluca/cf0/issues"
  },
  "homepage": "https://github.com/Taboneluca/cf0#readme",
  "devDependencies": {
    "@nx/eslint": "latest",
    "@nx/jest": "latest",
    "@nx/js": "latest",
    "@nx/linter": "latest",
    "@nx/next": "latest",
    "nx": "latest"
  }
}
