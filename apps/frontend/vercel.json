{
  "framework": "nextjs",
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": ".next",
  "ignoreCommand": "exit 0",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://api.cf0.ai/:path*" }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_SITE_URL": "https://www.cf0.ai"
  }
} 